{
    "manifestVersion": 1,
    "id": "OnDuty",
    "publisher": "ThiagoRiderAugusto",
    "version": "0.0.3",
    "name": "OnDuty Alerting and Events",
    "description": "Azure DevOps Extension for interacting with PagerDuty on pipelines",
    "public": true,
    "tags": [
        "PagerDuty",
        "Incident",
        "Alerting"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "icons": {
        "default": "img/extension-icon.jpg"        
    },
    "categories": ["Azure Pipelines"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "OnDuty"
            }
        }
    ],
    "files": [
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}